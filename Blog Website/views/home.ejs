<%- include partials/header -%> 
    
    <h1>Home</h1>
    <p> <%=Content%> </p>

    <% posts.forEach(element => {%> 
        <h1><%=element.title%></h1>
        <p> 
            <%=element.content.substring(0, 100) + "..." %>  
            <a href="/posts/<%= element._id %> ">Read More</a>
        </p>
    <%})%>
    
    
    <a href="/compose" class="btn btn-primary addButon">Add New Content</a>
    

<%- include partials/footer -%> 