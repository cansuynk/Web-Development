<%- include partials/header -%> 
    
    <h1>Compose</h1>
    

    <form action="/compose" method="post">
        <div class="form-group" >
            <label for="">Title</label> 
            <input type="text" name="composeTitle" class="form-control" autocomplete="off">

            <label>Post</label>
            <textarea type="text" name="composeBody" class="form-control" rows="4" autocomplete="off"></textarea>
        </div>
        <button type="submit" class="btn btn-primary addButon">Publish</button>
    </form>

<%- include partials/footer -%> 